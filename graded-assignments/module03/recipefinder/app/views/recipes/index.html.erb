<table>
	<tr>
		<th>Thumbnail</th>
		<th>Title</th>
		<th>Social Rank</th>
	</tr>
	<% @recipes.each do |recipe| %>
		<tr>
			<td><%= link_to image_tag(recipe["image_url"], width:"100", height:"100"), recipe["f2f_url"] %></td>
			<td><center><%= link_to sanitize(recipe["title"]), recipe["f2f_url"] %></center></td>
			<td><center><%= recipe["social_rank"] %></center></td>
		</tr>
	<% end %>	
</table>	